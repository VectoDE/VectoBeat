{
  "title": "VectoBeat Queue Health",
  "panels": [
    {
      "type": "timeseries",
      "title": "Queue Snapshot Age",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "targets": [
        {
          "expr": "queue_sync_snapshot_age_seconds{guild=~\"$guild\"}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "Queue Enqueue / Dequeue Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "targets": [
        {
          "expr": "rate(queue_sync_enqueued_total{guild=~\"$guild\"}[5m])",
          "legendFormat": "enqueue"
        },
        {
          "expr": "rate(queue_sync_dequeued_total{guild=~\"$guild\"}[5m])",
          "legendFormat": "dequeue"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "Queue Snapshot Size",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "targets": [
        {
          "expr": "queue_sync_snapshot_tracks{guild=~\"$guild\"}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Analytics Export Duration (p95)",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(analytics_export_duration_seconds_bucket[5m])) by (le))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "label": "Datasource"
      },
      {
        "name": "guild",
        "type": "query",
        "label": "Guild",
        "datasource": {
          "type": "prometheus",
          "uid": "$datasource"
        },
        "query": "label_values(queue_sync_snapshot_age_seconds, guild)",
        "refresh": 1
      }
    ]
  },
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "graphTooltip": 0
}